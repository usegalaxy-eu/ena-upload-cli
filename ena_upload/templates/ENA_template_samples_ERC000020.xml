import pandas as pd
import sys
def attributetest(row, column):
    if hasattr(row, column) and pd.notna(row[column]) and not str(row[column]).isspace() and str(row[column]).lower() not in ['nan', 'na']:
        return True 
    else:
        return False
def mandatorytest(row, column):
    if hasattr(row, column) and pd.notna(row[column]) and not str(row[column]).isspace() and str(row[column]).lower() not in ['nan', 'na']:
        return True 
    else:
        sys.exit(f"The mandatory column '{column}' is not filled in at row '{row}'")
